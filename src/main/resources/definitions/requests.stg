nick(nickname) ::= "NICK <nickname>\r\n"
user(username, realname) ::= "USER <username> * * :<realname>\r\n"
whois(nickname) ::= "WHOIS <nicknam>\r\n"
privmsg(nickname, message) ::= "PRIVMSG <nickname> :<message>\r\n"
notice(nickname, message) ::= "NOTICE <nickname> :<message>\r\n"
quit(message) ::= "QUIT :<message>\r\n"