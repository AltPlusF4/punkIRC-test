nick(nickname) ::= "NICK <nickname>"
user(username, realname) ::= "USER <username> * * :<realname>"
whois(nickname) ::= "WHOIS <nickname>"
privmsg(nickname, message) ::= "PRIVMSG <nickname> :<message>"
privmsgchannel(channel, message) ::= "PRIVMSG #<channel> :<message>"
notice(nickname, message) ::= "NOTICE <nickname> :<message>"
quit(message) ::= "QUIT :<message>"
join(channel) ::= "JOIN #<channel>"